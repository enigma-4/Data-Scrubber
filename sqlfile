-- Create a new database called 'datascrubber'
-- Connect to the 'master' database to run this snippet
USE master
GO
-- Create the new database if it does not exist already
IF NOT EXISTS (
    SELECT name
        FROM sys.databases
        WHERE name = N'datascrubber'
)
CREATE DATABASE datascrubber
GO

-- Create a new table called 'Patient' in schema 'dbo'
-- Drop the table if it already exists
IF OBJECT_ID('dbo.Patient', 'U') IS NOT NULL
DROP TABLE dbo.Patient
GO
-- Create the table in the specified schema
CREATE TABLE dbo.Patient
(
    PatientId INT NOT NULL PRIMARY KEY, -- primary key column
    Name [NVARCHAR](50) NOT NULL,
    Age [NVARCHAR](50) NOT NULL
    -- specify more columns here
);
GO
-- Insert rows into table 'dbo.Patient'
INSERT INTO dbo.Patient
( -- columns to insert data into
 [PatientId], [Name], [Age]
)
VALUES
   (1, N'John', 1),
   (2, N'Jane', 2),
   (3, N'Jack', 3),
   (4, N'Janet', 4)
-- add more rows here
GO
SELECT COUNT(*) as PatientCount FROM dbo.Patient;
SELECT e.PatientId ,e.Name, e.Age
FROM dbo.Patient as e
GO